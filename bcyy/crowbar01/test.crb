print("a.."+a+"\n");
if(a){
	print("true\n");
}else{
	print("false\n");
}
if(a||false){
	print("a || false\n");
}
if(a&&false){
	print("a || false\n");
}
if(true==true){
	print("true==true good\n");
}
if(true==false){
	print("true==false bad\n");
}
if(true!=true){
	print("true!=true bad\n");
}
if(true!=false){
	print("true!=false good\n");
}
